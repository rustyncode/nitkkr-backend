{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/notifications/scrape",
            "schedule": "0 0 * * *"
        }
    ]
}